<?xml version="1.0" encoding="utf-8"?>
<resources>
	<declare-styleable name="ProfileRow">
		<attr name="title" format="string" />
		<attr name="text_color" format="reference" />
		<attr name="subtitle" format="string" />
		<attr name="icon" format="reference" />
		<attr name="icon_color" format="reference" />
		<attr name="icon_size" format="dimension" />
		<attr name="line_visibility" format="boolean" />
		<attr name="switch_visibility" format="boolean" />
	</declare-styleable>

	<declare-styleable name="SegmentedProgressBar">
		<attr name="size" format="integer" />
		<attr name="active_count" format="integer" />
		<attr name="active_color" format="color" />
		<attr name="inactive_color" format="color" />
	</declare-styleable>

</resources>
